-- Table (only if you don't already have it)
CREATE TABLE books (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  title TEXT NOT NULL,
  dewey TEXT NOT NULL
);

-- Existing read query
selectAll:
SELECT id, title, dewey
FROM books
ORDER BY id DESC;

-- NEW: insert query
insertBook:
INSERT INTO books(title, dewey)
VALUES (?, ?);

-- (Optional) delete one
deleteBook:
DELETE FROM books WHERE id = ?;
